{
  "name": "shorpin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.13",
    "@types/linkifyjs": "^2.1.1",
    "@types/node": "^10.12.21",
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.11",
    "@types/react-swipeable": "^4.2.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "linkifyjs": "^2.1.8",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-scripts": "^2.1.3",
    "react-spring": "^8.0.1",
    "react-swipeable": "^5.0.1",
    "react-with-gesture": "^4.0.4",
    "typescript": "^3.3.1"
  },
  "scripts": {
    "startServer": "node --experimental-modules server/server.mjs",
    "startServerDev": "NODE_ENV=development node --experimental-modules server/server.mjs",
    "buildServer": "tsc src/server.ts --target es2018 --module es2015 --allowSyntheticDefaultImports --skipLibCheck --moduleResolution node && yarn run renameServer",
    "renameServer": "node scripts/renameServer.js",
    "buildAndDocker": "yarn run build && yarn run doTheDocker",
    "doTheDocker": "docker-compose build --no-cache server && docker-compose up",
    "doTheDockerOLD": "docker build -t shorpin .",
    "copyTheDockerToParent": "docker image save -o '../shorpin_server.tar' shorpin_server",
    "start": "react-scripts start",
    "build": "react-scripts build && yarn run buildServer",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
